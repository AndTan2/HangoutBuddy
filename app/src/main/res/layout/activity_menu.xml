<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical">


        <Button
            android:id="@+id/matches"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="goToMatches"
            android:text="Intalniri"/>

        <Button
            android:id="@+id/intalnire"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="goToCCOS"
            android:text="Gaseste Intalnire"/>

        <Button
            android:id="@+id/intalnirileMele"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="goToIntalnirileMele"
            android:text="Intalnirea Mea"/>

        <Button
            android:id="@+id/logOut"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="logoutUser"
            android:text="Log Out"/>


    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
